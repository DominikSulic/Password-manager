<Page x:Class="Pasword_Manager.loginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Pasword_Manager"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="loginPage">

    <Grid>
        <Label Content="ENTER MASTER PASSWORD" FontSize="20" Foreground="Aquamarine" HorizontalAlignment="Left" Margin="233,96,0,0" VerticalAlignment="Top" Height="59" Width="268"/>
        <TextBox x:Name="txtMasterPasswordInput" HorizontalAlignment="Left" Height="49" Margin="294,218,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="192"/>
       
        <Button x:Name="btnLogIn" Content="Log in" HorizontalAlignment="Left" Margin="347,296,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="1.093,0.1" Click="btnLogIn_Click"/>


    </Grid>
</Page>
